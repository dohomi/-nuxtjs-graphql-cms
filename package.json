{
  "name": "lumen-cms",
  "version": "0.0.2",
  "description": "Module for a cms based on Nuxt and GraphQl",
  "license": "MIT",
  "contributors": [
    {
      "name": "Dominic Garms <djgarms@gmail.com>"
    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/lumen-cms/lumen-cms",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev-fix": "nuxt test/fixture",
    "dev": "nuxt test/integration/article",
    "lint": "eslint --ignore-pattern '/lib/templates/plugins' --ext .js,.vue lib test",
    "test": "npm run lint && jest",
    "release": "standard-version && git push --follow-tags && npm publish",
    "build": "nuxt build test/integration/article",
    "start": "nuxt start",
    "analyze": "npm run build -- --a"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "dependencies": {
    "@babel/runtime-corejs2": "7.4.3",
    "@nuxtjs/apollo": "4.0.0-rc4",
    "@nuxtjs/axios": "5.3.6",
    "@nuxtjs/sitemap": "0.2.0",
    "accept-language-parser": "^1.5.0",
    "axios": "^0.18.0",
    "clone-deep": "4.0.1",
    "codemirror": "5.43.0",
    "cookie": "^0.3.1",
    "core-js": "2.6.5",
    "css-loader": "2.1.1",
    "graphql-tag": "2.10.1",
    "js-cookie": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.mergewith": "^4.6.1",
    "node-fetch": "2.3.0",
    "nuxt": "2.6.1",
    "nuxt-device-detect": "1.1.4",
    "quill": "^1.3.6",
    "slugify": "1.3.4",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "supports-webp": "2.0.1",
    "vue": "2.6.10",
    "vue-i18n": "8.10.0",
    "vue-ls": "^3.2.1",
    "vue-observe-visibility": "^0.4.3",
    "vuetify": "1.4.6",
    "vuetify-loader": "1.2.0"
  },
  "devDependencies": {
    "@vue/eslint-config-standard": "4.0.0",
    "babel-eslint": "10.0.1",
    "codecov": "3.3.0",
    "eslint": "5.16.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-vue": "5.2.2",
    "jest": "24.7.1",
    "jsdom": "14.0.0",
    "standard-version": "latest"
  },
  "engines": {
    "node": "10.x",
    "npm": "5.x"
  }
}
